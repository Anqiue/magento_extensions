<?xml version="1.0"?>
<config>
  <modules>
    <Fancye_Catalogurl>
      <version>0.1.0</version>
    </Fancye_Catalogurl>
  </modules>
  <frontend>
    <routers>
      <catalogurl>
        <use>standard</use>
          <args>
            <module>Fancye_Catalogurl</module>
            <frontName>catalogurl</frontName>
          </args>
      </catalogurl>
    </routers>
		<layout>
		  <updates>
			<catalogurl>
			  <file>catalogurl.xml</file>
			</catalogurl>
		  </updates>
		</layout>
  </frontend>
 <global>
  <helpers>
    <catalogurl>
      <class>Fancye_Catalogurl_Helper</class>
    </catalogurl>
  </helpers>
	<blocks>
	  <catalogurl>
		<class>Fancye_Catalogurl_Block</class>
	  </catalogurl>
	</blocks>
	<models>
	  <catalogurl>
		<class>Fancye_Catalogurl_Model</class>
		<resourceModel>catalogurl_mysql4</resourceModel>
	  </catalogurl>
			<catalog>
				<rewrite>
					<product>Fancye_Catalogurl_Model_Catalog_Product</product>
				</rewrite>
			</catalog>
	</models>
	<rewrite>
		<Fancye_Catalogurl_UrlproductController>			
	    <from><![CDATA[#^/p([0-9]+)-([a-zA-Z0-9\+\-\.])#]]></from>
            <to><![CDATA[catalogurl/urlproduct/view/]]></to>
			<complete>1</complete>
		</Fancye_Catalogurl_UrlproductController> 
  </rewrite> 
  <rewrite>   
   	<Fancye_Catalogurl_UrlcategoryController>			
	    <from><![CDATA[#^/c([0-9]+)-([a-zA-Z0-9\+\-\.])#]]></from>
            <to><![CDATA[catalogurl/urlcategory/view/]]></to>
			<complete>1</complete>
		</Fancye_Catalogurl_UrlcategoryController> 
 	</rewrite> 
 </global>
</config> 